    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    #parse("html/rdfa.vm")
    
    <link rel="stylesheet" type="text/css" media="screen,projection" href="http://ajax.googleapis.com/ajax/libs/jqueryui/1.8.10/themes/cupertino/jquery-ui.css" />
    <style type="text/css" media="screen,projection">
	    #include("css/base.css")
	    #include("css/custom.css")
		#include("css/SemEx.css")
		#if (${profile} == "business")
			#include("css/${profile}.css")
		#end
		#if (${profile} == "technical")
			#include("css/${profile}.css")
		#end
    </style>
 
    <link rel="stylesheet" type="text/css" media="screen,projection" href="report/jixedbar/jx.stylesheet.css" /> 
    <link type="image/x-icon" href="images/favicon.ico" rel="shortcut icon" />
