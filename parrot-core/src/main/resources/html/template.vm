  <script type="text/javascript" src="https://ajax.googleapis.com/ajax/libs/jquery/1.5.1/jquery.min.js"></script> 
  <script type="text/javascript" src="https://ajax.googleapis.com/ajax/libs/jqueryui/1.8.10/jquery-ui.min.js"></script> 
  <script type="text/javascript" src="report/js/jquery.corner.js"></script> 
  <script type="text/javascript" src="report/jixedbar/jquery.jixedbar.min.js"></script> 
  <script type="text/javascript" src="report/js/jquery.qtip-1.0.js"></script> 
