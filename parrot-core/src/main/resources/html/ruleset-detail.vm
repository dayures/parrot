<div class="term">
  <h3><a id="$element.Anchor"></a><span class="parrot-element-title parrot-ruleset-title">$element.Label</span> (RuleSet)</h3>
  <dl>
   #if($element.Strategy)
      <dt>Strategy</dt>
      <dd>$element.Strategy</dd>
   #end

   #if($element.Priority)
      <dt>Priority</dt>
      <dd>$element.Priority</dd>
   #end
	   
   #if($element.Uri)
      <dt>URI</dt>
      <dd><a href="$element.Uri">$element.Uri</a></dd>      

	   #if($element.Comment)
	      <dt>Description</dt>
	      <dd>$element.Comment</dd>
	   #end
	
	   #if(!$element.creators.isEmpty())
	   <dt>Creators:</dt>
	   #foreach($creator in $element.creators)
	     <dd>$creator</dd>
	   #end
	   #end
	   
	   #if(!$element.contributors.isEmpty())
	   <dt>Contributors:</dt>
	   #foreach($contributor in $element.contributors)
	     <dd>$contributor</dd>
	   #end
	   #end
	   
	   #if(!$element.publishers.isEmpty())
	   <dt>Publishers:</dt>
	   #foreach($publisher in $element.publishers)
	     <dd>$publisher</dd>
	   #end
	   #end
	   
	   #if($element.Date)
	      <dt>Date</dt>
	      <dd>$element.Date</dd>
	   #end
	   
   #end   
   
   #if(!$element.referencedOntologicalObjects.isEmpty())
   <dt>Referenced model objects</dt>
   #foreach($v in $element.referencedOntologicalObjects)
	 <dd><a href="#$v.LocalName">$v.Label</a></dd>
   #end
   #end
   
   #if(!$element.rules.isEmpty())
   <dt>Rules</dt>
   #foreach($v in $element.rules)
	 <dd><span class="parrot-element-ref parrot-rule-ref"><a href="#$v.LocalName">$v.Label</a></span></dd>
   #end
   #end
   
   #if(!$element.ruleSets.isEmpty())
   <dt>RuleSets</dt>
   #foreach($v in $element.ruleSets)
	 <dd><a href="#$v.LocalName">$v.Label</a></dd>
   #end
   #end
   
   #if($element.Parent)
      <dt>is inside of</dt>
      <dd><a href="#$element.Parent.LocalName">$element.Parent.Label</a></dd>
   #end
   
   #if(!$element.labels.isEmpty())
   <dt>Labels</dt>
   <dd>
   #parse("html/label.vm")
   </dd>
   #end
   
   #if(!$element.relatedDocuments.isEmpty())
   <dt>Related Documents</dt> 
   <dd>
     #foreach($element in $element.relatedDocuments)
       #parse("html/relatedDocument-detail.vm")
     #end
   </dd>
   #end
  
  </dl>
  <p class="back-to-top">[<a href="#toc">back to top</a>]</p> 
</div>
  