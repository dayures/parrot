<div class="term">
  <h3><a id="$element.Anchor"></a><span class="parrot-element-title parrot-rule-title">$element.Label</span> (Rule)</h3>
  <dl>
   #if($element.Uri)
      <dt>URI</dt>
      <dd><a href="$element.Uri">$element.Uri</a></dd>      
	   #if($element.Comment)
	      <dt>Description</dt>
	      <dd>$element.Comment</dd>
	   #end
	   
	   #if(!$element.creators.isEmpty())
	   <dt>Creators:</dt>
	   #foreach($creator in $element.creators)
	     <dd>$creator</dd>
	   #end
	   #end
	   
	   #if(!$element.contributors.isEmpty())
	   <dt>Contributors:</dt>
	   #foreach($contributor in $element.contributors)
	     <dd>$contributor</dd>
	   #end
	   #end
	   
	   #if(!$element.publishers.isEmpty())
	   <dt>Publishers:</dt>
	   #foreach($publisher in $element.publishers)
	     <dd>$publisher</dd>
	   #end
	   #end
	   
	   #if($element.Date)
	      <dt>Date</dt>
	      <dd>$element.Date</dd>
	   #end

   #end
   
   #if(!$element.referencedOntologicalObjects.isEmpty())
   <dt>Referenced model objects</dt>
   #foreach($reference in $element.referencedOntologicalObjects)
	 <dd><span class="parrot-element-ref parrot-${reference.kindString}-ref"><a href="#$reference.localName">$reference.label</a></span></dd>
   #end
   #end
   
   #if($element.Parent)
      <dt>is inside of</dt>
      <dd><span class="parrot-element-ref parrot-${element.Parent.kindString}-ref"><a href="#$element.Parent.LocalName">$element.Parent.Label</a></span></dd>
   #end
   
   #if(!$element.labels.isEmpty())
   <dt>Labels</dt>
   <dd>
   #parse("html/label.vm")
   </dd>
   #end
   
   #if(!$element.relatedDocuments.isEmpty())
   <dt>Related Documents</dt> 
   <dd>
     #foreach($element in $element.relatedDocuments)
       #parse("html/relatedDocument-detail.vm")
     #end
   </dd>
   #end
  
  </dl>
</div>
  