<div class="term">
  <h3><a id="$element.Anchor"></a><span class="parrot-property-title">$element.Label</span> (Property)</h3>
  <dl>
   #if($element.Uri)
      <dt>Uri</dt>
      <dd><a href="$element.Uri">$element.Uri</a></dd>      
   #end
   
      <dt>Property Type</dt>
      <dd>$element.Type</dd>
   
   
   #if($element.Comment)
      <dt>Description</dt>
      <dd>$element.Comment</dd>
   #end

   #if($element.Domain)
      <dt>Domain</dt>
   	  <dd><span class="parrot-class-ref"><a href="#$element.Domain.LocalName">$element.Domain.Label</a></span></dd>
   #end

   #if($element.Range)
      <dt>Range</dt>
      <dd><span class="parrot-class-ref"><a href="#$element.Range.LocalName">$element.Range.Label</a></span></dd>
   #end
   
   #if(!$element.SuperProperties.isEmpty())
    <dt>SubProperty of</dt>
    #foreach($superproperty in $element.SuperProperties)
      <dd><span class="parrot-property-ref"><a href="#$superproperty.LocalName">$superproperty.Label</a></span></dd>
    #end
   #end

   #if(!$element.SubProperties.isEmpty())
    <dt>SuperProperty of</dt>
    #foreach($subproperty in $element.SubProperties)
      <dd><span class="parrot-property-ref"><a href="#$subproperty.LocalName">$subproperty.Label</a></span></dd>
    #end
   #end

   #if(!$element.InverseReferences.isEmpty())
   <dt>Referenced from</dt>
   #foreach($reference in $element.InverseReferences)
     <dd><a href="#$reference.LocalName">$reference.Label</a></dd>
   #end
   #end

   #if($element.cardinality != 0)
   <dt>Cardinality:</dt>
   <dd>$element.cardinality</dd>
   #end
   
   #if(!$element.depictions.isEmpty() || !$element.videos.isEmpty())
   <dt>Multimedia</dt>
   #end
   
   #if(!$element.depictions.isEmpty())
   #foreach($depiction in $element.depictions)
     <dd><img src="$depiction" alt="" /></dd>
   #end
   #end
   
   #if(!$element.videos.isEmpty())
   #foreach($video in $element.videos)
     <dd>
     <object data="$video">
		<param name="movie" value="$video" />
	</object>
	</dd>
   #end
   #end
   
   #if(!$element.inverseRuleReferences.isEmpty())
   <dt>Referenced rules</dt>
   #foreach($reference in $element.inverseRuleReferences)
     <dd><span class="parrot-rule-ref"><a href="#$reference.LocalName">$reference.Label</a></span></dd>
   #end
   #end
   
  </dl>
  <p class="back-to-top">[<a href="#toc">back to top</a>]</p> 
</div>   