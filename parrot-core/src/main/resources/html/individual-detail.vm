<div class="term">
  <h3><a id="$element.Anchor"></a><span class="parrot-individual-title">$element.Label</span> (Individual)</h3>
  <dl>
  #if($element.Uri)
      <dt>Uri</dt>
      <dd><a href="$element.Uri">$element.Uri</a></dd>      
  #end

  #if(!$element.types.isEmpty())
    <dt>Types</dt>
    #foreach($type in $element.types)
     <dd><span class="parrot-class-ref"><a href="#$type.LocalName">$type.Label</a></span></dd>
    #end
  #end
  
  #if(!$element.depictions.isEmpty())
   <dt>Multimedia</dt>
   #foreach($depiction in $element.depictions)
     <dd><img src="$depiction" alt="" /></dd>
   #end
   #end
  
  #if(!$element.inverseRuleReferences.isEmpty())
   <dt>Referenced rules</dt>
   #foreach($reference in $element.inverseRuleReferences)
	 <dd><span class="parrot-rule-ref"><a href="#$reference.LocalName">$reference.Label</a></span></dd>
   #end
  #end
  
  </dl>
  <p class="back-to-top">[<a href="#toc">back to top</a>]</p> 
</div>   