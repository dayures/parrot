#if ( ($element.Version) ||
	  ($element.Date) ||
	  (!$element.creators.isEmpty()) ||
	  (!$element.contributors.isEmpty()) ||
	  (!$element.publishers.isEmpty()) ||
	  ($element.Rights) ||
	  ($element.LicenseLabel) )
  #set( $vci = true )
#else
  #set( $vci = false )
#end


#if ( ($element.Uri) )
  #set( $ti = true )
#else
  #set( $ti = false )
#end


#if ( (!$element.referencedOntologicalObjects.isEmpty()) ||
  	  ($element.Parent) )
  #set( $inuse = true )
#else
  #set( $inuse = false )
#end

#if ( (!$element.labels.isEmpty()) ||
  	  (!$element.relatedDocuments.isEmpty()) )
  #set( $otherinfo = true )
#else
  #set( $otherinfo = false )
#end

<div class="term">
  <h3><a id="$element.Anchor"></a><span class="parrot-element-title parrot-rule-title">$element.Label</span> (Rule)
    #parse("html/detail-menu.vm")
  </h3>
  #parse("html/description.vm")
  
  <div>

  #if ($vci)
    #parse("html/version-control.vm")
  #end
    
  
  #if ( $ti )
  <div class="ti"> 
  <h4>Definition</h4> 
  <dl>
    #if($element.Uri)
      <dt>URI</dt>
      <dd><pre>$element.Uri</pre></dd>
    #end
  </dl>
  </div>
  #end
  
  #if ( $inuse )
  <div class="inuse"> 
  <h4>In use</h4> 
  <dl>   
    #if(!$element.referencedOntologicalObjects.isEmpty())
      <dt>Referenced model objects</dt>
      #foreach($reference in $element.referencedOntologicalObjects)
	  <dd><span class="parrot-element-ref parrot-${reference.kindString}-ref"><a href="#$reference.localName">$reference.label</a></span></dd>
      #end
    #end

    #if($element.Parent)
      <dt>is inside of</dt>
      <dd><span class="parrot-element-ref parrot-${element.Parent.kindString}-ref"><a href="#$element.Parent.LocalName">$element.Parent.Label</a></span></dd>
    #end
    
  </dl>
  </div>
  #end
  
  #if ( $otherinfo )
  	#parse("html/documentation.vm")
  #end
  
  </div>
</div>
  