<div class="term">
  #if($element.Label)
    <h3><a id="$element.Anchor"></a>Rule: $element.Label</h3>
  #else
    <h3><a id="$element.Anchor"></a>Rule: $element.Uri</h3>
  #end
  <dl>
  #if($element.Uri)
      <dt>Uri</dt>
      <dd><a href="$element.Uri">$element.Uri</a></dd>      
  #end
  #if($element.Label)
      <dt>Label</dt>
      <dd>$element.Label</dd>
   #end
   #if($element.Comment)
      <dt>Comment</dt>
      <dd>$element.Comment</dd>
   #end
   #if($element.Date)
      <dt>Date</dt>
      <dd>$element.Date</dd>
   #end
    #if(!$element.ReferencedOntologyProperties.isEmpty())
   <dt>Referenced ontology properties</dt>
   #foreach($v in $element.ReferencedOntologyProperties)
      <dd><a href="#$v.LocalName">$v.Identifier</a></dd>
   #end
  #end

    #if(!$element.DeclaredVars.isEmpty())
   <dt>Declared variables</dt>
   #foreach($v in $element.DeclaredVars)
      <dd>$v</dd>
   #end
  #end
  
  </dl>
</div>
  